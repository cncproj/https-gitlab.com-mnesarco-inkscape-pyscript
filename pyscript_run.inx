<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <_name>Run and Update</_name>
    <id>com.fdmtech.inkscape.pyscript.run</id>
    <dependency type="executable" location="extensions">pyscript_run.py</dependency>
    <dependency type="executable" location="extensions">inkex.py</dependency>
    <effect>
      <effects-menu>
        <submenu _name="Python Scripting"/>
      </effects-menu>
    </effect>
    <script>
        <command reldir="extensions" interpreter="python">pyscript_run.py</command>
    </script>
</inkscape-extension>
